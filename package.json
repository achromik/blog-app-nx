{
  "name": "blog-app-nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run mongo:dev:start && nx run-many --all --target=serve --parallel && npm run mongo:dev:stop",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx workspace-lint && npx nx run-many --all --target=lint --parallel",
    "image:builder": "docker build -t blog-app-nx:base .",
    "build:docker:dev": "docker-compose -f docker-compose.dev.yml build",
    "build:docker:prod": "docker-compose build",
    "mongo:dev:start": "docker-compose -f docker-compose.dev.yml up -d mongo",
    "mongo:dev:stop": "docker-compose -f docker-compose.dev.yml stop mongo",
    "docker:dev:start": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod:start": "docker-compose up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.dev.yml stop",
    "docker:prod:stop:": "docker-compose stop",
    "dev": "npm run docker:dev:start && npm run docker:dev:stop"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^7.0.0",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/platform-express": "^7.0.0",
    "bcrypt": "^5.0.1",
    "core-js": "^3.6.5",
    "mongoose": "^5.12.4",
    "nx": "^12.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.10.0",
    "@nrwl/cypress": "12.10.0",
    "@nrwl/eslint-plugin-nx": "12.10.0",
    "@nrwl/jest": "12.10.0",
    "@nrwl/linter": "12.10.0",
    "@nrwl/nest": "^12.10.0",
    "@nrwl/node": "12.10.0",
    "@nrwl/react": "12.10.0",
    "@nrwl/tao": "12.10.0",
    "@nrwl/web": "12.10.0",
    "@nrwl/workspace": "12.10.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "7.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "babel-jest": "27.2.3",
    "cypress": "^8.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  }
}
